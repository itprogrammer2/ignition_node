	<div class="modal fade" id="LoginModal" tabindex="-1" role="dialog" aria-labelledby="LoginModalLabel" aria-hidden="true">
	  	<div class="modal-dialog">
		    <div class="modal-content">
		      	<div class="modal-body">
				    <div class="row">
				        <div class="col-lg-8 col-sm-6 col-md-8 col-md-offset-2">
				            <div class="account-wall">
				            	<p class="text-center login-title">
				            		To continue, sign in to &nbsp;</p>
				                <!-- <img class="profile-img" src="../images/nophoto.png"
				                    alt=""> -->
				                <img src="../images/FINAL_LOGO1.png" style="width: 80%; margin: 0 0 0 10%;">
				                <form class="form-signin" action="/user/auth" method="post">
					                <input type="text" class="form-control" placeholder="Email" name="username" required autofocus>
					                <input type="password" class="form-control" placeholder="Password" name="password" required>
					                <button class="btn btn-lg btn-danger btn-block" type="submit">
					                    Sign in</button>
					                <label class="checkbox pull-left">
					                    <input type="checkbox" value="remember-me">
					                    Remember me
					                </label>
					                <a href="#" class="pull-right need-help">Need help? </a><span class="clearfix"></span>

					                <!-- <a href="register.html" class="text-center new-account" >Create an account </a> -->

					                <div class="row">
								        <div class="col-xs-12 col-sm-6 col-md-6" style="margin-top: 5px;">
								          	<a href="#" class="btn btn-sm btn-facebook btn-block">Sign in with Facebook</a>
								        </div>
								        <div class="col-xs-12 col-sm-6 col-md-6" style="margin-top: 5px;">
								          	<a href="#" class="btn btn-sm btn-google btn-block">Sign in with Google</a>
								        </div>
								    </div>
								    <div class="form-group" style="margin-top:10px;">
	                                    <div class="col-md-12 control">
	                                        <div style="border-top: 1px solid#888; padding-top:15px; font-size:85%" >
	                                            Don't have an account! 
	                                        <a href="/user/register" onClick="$('#loginbox').hide(); $('#signupbox').show()">
	                                            Sign Up Here
	                                        </a>
	                                        </div>
	                                    </div>
	                                </div>   
				                </form>
				            </div>
				        </div>
				    </div>
		      	</div>
		    </div>
	  	</div>
